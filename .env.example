# Zoho OAuth Configuration
# Replace these values with your actual Zoho OAuth credentials
VITE_ZOHO_CLIENT_ID=your_zoho_client_id_here
VITE_ZOHO_CLIENT_SECRET=your_zoho_client_secret_here

# Redirect URI for OAuth flow completion
VITE_ZOHO_REDIRECT_URI=http://localhost:8080/callback

# Optional: Override the base OAuth URL if needed (default is already set in code)
# VITE_ZOHO_OAUTH_URL=https://accounts.zoho.com/oauth/v2/auth

# Optional: Override the scopes if needed (default is already set in code)
# VITE_ZOHO_SCOPES=ZohoBooks.invoices.READ,ZohoBooks.invoices.UPDATE,ZohoBooks.contacts.READ

# OAuth token endpoints - to be used by backend for token exchange
VITE_ZOHO_TOKEN_URL=https://accounts.zoho.in/oauth/v2/token
VITE_ZOHO_REFRESH_TOKEN_URL=https://accounts.zoho.in/oauth/v2/token

# AWS Cognito Configuration
VITE_AWS_REGION=ap-south-1
VITE_COGNITO_USER_POOL_ID=your-user-pool-id
VITE_COGNITO_CLIENT_ID=your-app-client-id

# API Configuration
VITE_API_BASE_URL=http://localhost:8000/api/v1
